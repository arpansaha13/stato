{
  "name": "awast",
  "packageManager": "pnpm@7.0.0",
  "private": true,
  "version": "0.0.0",
  "author": "Arpan Saha <https://github.com/lemontreejs>",
  "description": "A lightweight tool for UI development for vue3.",
  "type": "module",
  "bin": "./bin/awast.mjs",
  "license": "ISC",
  "homepage": "https://github.com/lemontreejs/awast",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemontreejs/awast"
  },
  "bugs": {
    "url": "https://github.com/lemontreejs/awast/issues"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --open",
    "typecheck": "vue-tsc --noEmit",
    "build": "pnpm typecheck && unbuild && node esbuild.config.js",
    "clean": "if exist dist rd dist /s /q",
    "prepack": "pnpm clean && pnpm build && node scripts/prepack.mjs",
    "package": "pnpm prepack && cd dist && pnpm pack"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "vite": "^2.9.9",
    "vue": "^3.2.36"
  },
  "devDependencies": {
    "@types/node": "^17.0.36",
    "@types/normalize-path": "^3.0.0",
    "@vueuse/core": "^8.5.0",
    "esbuild": "^0.14.42",
    "fast-glob": "^3.2.11",
    "mri": "^1.2.0",
    "normalize-path": "^3.0.0",
    "typescript": "^4.7.2",
    "unbuild": "^0.7.4",
    "vue-tsc": "^0.34.17"
  },
  "peerDependencies": {
    "vue": "^3.2.36"
  },
  "keywords": [
    "awast",
    "vue",
    "components"
  ]
}