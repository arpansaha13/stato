{
  "name": "@stato/vue",
  "packageManager": "pnpm@7.1.9",
  "private": true,
  "version": "0.0.0",
  "author": "Arpan Saha <https://github.com/lemontreejs>",
  "description": "A lightweight tool for vue3 for developing components in isolation.",
  "type": "module",
  "bin": {
    "stato": "./bin/stato.mjs"
  },
  "license": "ISC",
  "homepage": "https://github.com/lemontreejs/stato",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemontreejs/stato"
  },
  "bugs": {
    "url": "https://github.com/lemontreejs/stato/issues"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --open",
    "typecheck": "vue-tsc --noEmit",
    "build": "pnpm typecheck && node unbuild.js && node esbuild.js",
    "clean": "if exist dist rd dist /s /q",
    "prepack": "pnpm clean && pnpm build && node scripts/prepack.mjs",
    "package": "pnpm prepack && cd dist && pnpm pack"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "vite": "^2.9.9",
    "vue": "^3.2.36"
  },
  "devDependencies": {
    "@headlessui/vue": "^1.6.4",
    "@types/node": "^17.0.36",
    "@types/rimraf": "^3.0.2",
    "@vueuse/core": "^8.5.0",
    "change-case": "^4.1.2",
    "esbuild": "^0.14.42",
    "fast-glob": "^3.2.11",
    "mri": "^1.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.2",
    "unbuild": "^0.7.4",
    "vue-tsc": "^0.34.17"
  },
  "peerDependencies": {
    "vue": "^3.2.36"
  },
  "keywords": [
    "stato",
    "vue",
    "components"
  ]
}